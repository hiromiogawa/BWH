<% if logged_in? %>
  <div id="login"><%= image_tag('common/login.png') %></div>
  <%= render 'users/userdata' %>
<% else %>
  <div class="btn-wrap center">
      <div class="btn"><%= link_to '新規登録', signup_path %></div>
      <div class="btn"><%= link_to 'ログイン', login_path %></div>
  </div>
<% end %>
